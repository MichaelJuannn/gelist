// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Event {
  id        Int @id @default(autoincrement())
  slug      String @unique
  title     String
  date      String
  members   Json
  category  String
}
